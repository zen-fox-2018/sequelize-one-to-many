<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Page Title</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
  <script src="main.js"></script>
</head>
<body>
    <form action="/teacher/edit=<%=data.id%>" method="POST" >
      First name:<br>
      <input type="text" name="first_name" value="<%=data.first_name%>">
      <br>
      Last name:<br>
      <input type="text" name="last_name" value="<%=data.last_name%>">
      <br>
      E-mail:<br>
      <input type="text" name="email" value="<%=data.email%>">
      <br><br>
      subject: <br>
  
      <select name = "SubjectId" >
        <% dataSubject.forEach(element =>{ %>
          <%if(data.SubjectId == element.id){%>
            <option selected = "selected" value="<%= element.id%>"> <%= element.subject_name %></option>
          <%} else{ %>
            <option value="<%= element.id%>"> <%= element.subject_name %></option>
          <% } %>
        <% }) %>
      </select>
      <br><br>
      <br><br>
      <input type="submit" value="Submit">
  </form>
</body>
</html>
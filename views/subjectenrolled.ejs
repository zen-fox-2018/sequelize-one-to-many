<!DOCTYPE html>
<html lang="en">
<%- include('./partials/header.ejs') %>
<body>
<%- include('./partials/navbar.ejs') %>
  
<div class="container-fluid text-center">    
  <div class="row content">
    <div class="col-sm-2 sidenav">
      <p><a href="#"></a></p>
      <p><a href="#"></a></p>
      <p><a href="#"></a></p>
    </div>
    <div class="col-sm-8 text-left"> 
      <center><h1><%= subject.subjectName %></h1></center> 
      <center><a href="/subjects">return to subject list</a></center>
      <table style="width:100%" border="1">
        <tr>
          <th>Name</th>
          <th>Score</th>
          <th>Score By Letter</th>
        </tr>
       <tr>
         <%subject.Students.forEach(dataStudent => { %>
            <tr>
              <td><%= `${dataStudent.firstName} ${dataStudent.lastName}` %></td>
          
        <td> <%- getEnrolled(dataStudent) %></td>

          
            <td> <%= getGrade(dataStudent.StudentSubject.score) %></td>
         <% }) %>
       
       </tr>
      </table>
     </div>


     <%- include('./partials/footer.ejs') %>

</body>
</html>

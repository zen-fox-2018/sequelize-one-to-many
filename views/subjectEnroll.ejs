<%- include('./partials/header.ejs') %>
<%- include('./partials/sidebar.ejs') %>
<%- include('./partials/mainHeader.ejs') %>
                        <div class="row">
                            <!-- Textual inputs start -->
                            <div class="col-12 mt-5">
                                <div class="card">
                                  <div class="card-body">
                                    <h4 class="header-title"><%= title %></h4>
                                    <div class="single-table">
                                        <div class="table-responsive">
                                            <table class="table text-center">
                                                <thead class="text-uppercase bg-light">
                                                    <tr>
                                                        <th scope="col">ID</th>
                                                        <th scope="col">Student Name</th>
                                                        <th scope="col">Score</th>
                                                        <th scope="col">Score By Letter</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                  <% if (subject.Students.length) { %>
                                                    <% subject.Students.forEach( (students, i) => { %>
                                                      <tr>
                                                        <th scope="row"><%= i+1 %></th>
                                                        <td><%= students.firstName %> <%= students.lastName %></td>
                                                        <td>
                                                          <%- giveScore(students.studentSubject.score) %>
                                                        </td>
                                                        <td><%= scoreByLetter(students.StudentSubject.score) %></td>
                                                      </tr>
                                                    <% }) %>
                                                  <% } else { %>
                                                        <tr>
                                                          <td colspan="3">Empty</td>
                                                        </tr>
                                                  <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                  </div>
                                </div>
                            </div>
                            <!-- Textual inputs end -->
                        </div>
<%- include('./partials/footer.ejs') %>
<%- include('./partials/script.ejs') %>
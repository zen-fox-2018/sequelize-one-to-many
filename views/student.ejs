<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Student</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
  <script src="main.js"></script>
</head>
<body>
  <h1>Students Data</h1>
  <p><a href="/student/add">Add Student</a></p>
  <table>
      <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
        <th>Options</th>
      </tr>
      
      <% data.forEach(element=>{%>
        <tr>
          <td><%= element.id %></td>
          <td><%= element.first_name %></td>
          <td><%= element.last_name %></td>
          <td><%= element.email %></td>
          <td><a href= "/student/<%=element.id%>/add-subject">Add Subject</a></td>
          <td> <a href="/student/edit/<%=element.id%>">edit</a> || <a href= "/student/delete/<%= element.id %>" onclick="confirm ('delete this student?')">delete</a></td>
        </tr>
        <% })%>
    </table>
</body>
</html>
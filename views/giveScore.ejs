<%- include('./partials/header.ejs') %>
<%- include('./partials/sidebar.ejs') %>
<%- include('./partials/mainHeader.ejs') %>                <div class="row justify-content-center">
                  <!-- Textual inputs start -->
                  <div class="col-lg-6 mt-5">
                      <div class="card">
                          <div class="card-body">
                              <h4 class="header-title"><%= title %></h4>
                              <form action="/subjects/<%= id %>/give-score" method="post">
                                <div class="form-group">
                                  <label for="example-text-input" class="col-form-label">Student Name</label>
                                  <input class="form-control" value="<%= studentName %>" type="text" id="studentName" name="studentName" disabled>
                                </div>
                                <div class="form-group">
                                  <label for="example-text-input" class="col-form-label">Subject Name</label>
                                  <input class="form-control" value="<%= subjectName %>" type="text" id="subjectName" name="subjectName" disabled>
                                </div>
                                <div class="form-group">
                                  <label for="example-text-input" class="col-form-label">Score</label>
                                  <input class="form-control" type="text" id="score" name="score">
                                </div>
                                <div class="form-group pull-right">
                                  <button type="submit" class="btn btn-primary mb-3">Submit</button>
                                  <button type="cancel" class="btn btn-danger mb-3">Cancel</button>
                              </div>
                              </form>
                        </div>
                      </div>
                  </div>
                  <!-- Textual inputs end -->
                </div>
<%- include('./partials/footer.ejs') %>
<%- include('./partials/script.ejs') %>
